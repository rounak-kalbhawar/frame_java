package com.mycompany.frame;
import javax.swing.*; 
/**
 *
 * @author USER
 */
public class test2 extends JFrame 
{ 
	JFrame frame; 
	frame_inherit() 
	{ 
		setTitle("by inheritance"); 
		setSize(400, 500); 
		setLayout(null); 
		setVisible(true); 
	} 
	
	public static void main(String[] args) 
	{ 
		new frame_inherit(); 
	} 
} 
